{
"manifest_version": 2,
    "name": "Millionaire MPs",
    "description": "Ensure any mentions of MPs in web pages are qualified with their millionaire status",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "128":"logo.png"
    }
}